 $( ".search-width" ).autocomplete({
    source:source_array
    });
  $(document).ready(function () {
    /*$(window).scroll(function(){
      ScrollExecute();
    });*/
   
    $('body').on('click', '#more a', function (event) {
      event.preventDefault();
      ScrollExecute();
    })
  });
</script>
