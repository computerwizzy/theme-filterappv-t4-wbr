<script>
  let triggered = false;

  function ScrollExecute() {
    let moreButon = $('#more').last();
    let nextUrl = $(moreButon).find('a').attr("href");
    if ((($(moreButon).offset().top - $(window).scrollTop()) < 800) && (triggered == false)) {
      triggered = true;
      $.ajax({
          url: nextUrl,
          type: 'GET',
          beforeSend: function () {
            moreButon.remove();
          }
        })
        .done(function (data) {
          $('.product').append($(data).find('.product').html());
          triggered = false
        });
    }
  }
